{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "viescloud-utils": {
      "projectType": "library",
      "root": "projects/viescloud-utils",
      "sourceRoot": "projects/viescloud-utils/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/viescloud-utils/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/viescloud-utils/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/viescloud-utils/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "projects/viescloud-utils/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/viescloud-utils/public"
              }
            ],
            "styles": [
              "projects/viescloud-utils/src/styles.scss"
            ]
          }
        }
      }
    },
    "skeleton": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/skeleton",
      "sourceRoot": "projects/skeleton/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "browser": "projects/skeleton/src/main.ts",
            "tsConfig": "projects/skeleton/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/skeleton/public"
              }
            ],
            "styles": [
              "projects/skeleton/src/styles.scss"
            ],
            "server": "projects/skeleton/src/main.server.ts",
            "outputMode": "server",
            "ssr": {
              "entry": "projects/skeleton/src/server.ts"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "999mb",
                  "maximumError": "999mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "999mb",
                  "maximumError": "999mb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "skeleton:build:production"
            },
            "development": {
              "buildTarget": "skeleton:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "projects/skeleton/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/skeleton/public"
              }
            ],
            "styles": [
              "projects/skeleton/src/styles.scss"
            ]
          }
        }
      }
    }
  }
}
